{
  "name": "system-dashboard",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "c8 -x 'tests/**/*.mjs' --temp-directory build/tmp ava && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme {{module}} --section=API",
    "lint": "documentation lint {{module}}",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "test": "ava"
  },
  "dependencies": {},
  "devDependencies": {
    "ava": "^2.4.0",
    "builtin-modules": "^3.1.0",
    "c8": "^5.0.4",
    "documentation": "^12.1.2",
    "esm": "^3.2.25",
    "markdown-doctest": "^1.0.0",
    "rollup": "^1.24.0",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-executable": "^1.5.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "semantic-release": "^15.13.26"
  },
  "engines": {
    "node": ">=12.12.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/system-dashboard.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/system-dashboard/issues"
  },
  "homepage": "https://github.com/arlac77/system-dashboard#readme",
  "ava": {
    "require": [
      "esm"
    ],
    "extensions": [
      "mjs"
    ],
    "babel": false,
    "compileEnhancements": false,
    "files": [
      "tests/*-test.mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}